<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class 
   name="com.org.yobox.model.State" 
   table="state" 
   >


<id  name="id"   column="id"    type="java.lang.Integer"   >
<generator class="identity"></generator>
</id>


<property
            name="countryId"
            type="java.lang.Integer"
            update="true"
            insert="true"
            column="country_id"
            
/>
    
    
<property
            name="name"
            type="java.lang.String"
            update="true"
            insert="true"
            column="name"
            length="128"
    />
    
    <property
            name="code"
            type="java.lang.String"
            update="true"
            insert="true"
            column="code"
            length="32"
    />
    
<property
            name="activeStatus"
            type="boolean"
            update="true"
            insert="true"
            column="active_status"
            
/>

    
    
</class>
</hibernate-mapping>