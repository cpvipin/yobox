<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class 
   name="com.org.yobox.model.NewsLetter"
   table="newsletter">


<id  name="id"   column="id"    type="java.lang.Integer"   >
<generator class="identity"></generator>
</id>

      
    
   
   
<property
            name="email"
            type="java.lang.String"
            update="true"
            insert="true"
            column="email"
            length="96"
            unique="true"
    />
    
    
    
   
     
<property
            name="activeStatus"
            type="boolean"
            update="true"
            insert="true"
            column="active_status"
            
/>
   
  
   

<property
            name="dateAdded"
            type="java.util.Date"
            update="true"
            insert="true"
            column="date_added"          
/>

 
<property
            name="dateModified"
            type="java.util.Date"
            update="true"
            insert="true"
            column="date_modified"
            
/>


    

</class>

</hibernate-mapping>